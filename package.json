{
  "name": "typing-web-app",
  "version": "0.1.0",
  "homepage": "https://tundrekka.github.io/Typing-Web-App",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "firebase": "^8.6.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "vite": "^6.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "dev": "vite",
    "all": "vite --host 192.168.0.104",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build-css": "sass src/styles/styles.scss src/styles/styles.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "space-in-parens": [
        "warn",
        "always",
        {
          "exceptions": [
            "{}"
          ]
        }
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "array-bracket-spacing": [
        "warn",
        "always",
        {
          "singleValue": false
        }
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "no-multiple-empty-lines": "warn",
      "eol-last": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.0"
  }
}
